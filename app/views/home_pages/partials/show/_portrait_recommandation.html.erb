<div class="col-xs-12 recommandation">
  <!-- wrapper link -->
  <%= link_to portrait_path(portrait[:item_id]) do %>
    <div class="img-wrapper">
      <!-- image -->
      <%= image_tag(portrait[:media], alt: "") %>
      <div class="layer"></div>
    </div>
    <!-- name -->
    <h3>
      <%= portrait[:title] %>
    </h3>

  <% end %>
</div>
